{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars":"warn",
      "no-console": "off",
      "func-names":"off",
      "object-shorthand":"off",
      "class-methods-use-this":"off"
    }
}
